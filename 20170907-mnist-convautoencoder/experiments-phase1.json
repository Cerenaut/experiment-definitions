            },
            {
              "entity-name": "cnn",
              "parameter-path": "momentum",
              "val-series": [0.5, "*"]
            },
            {
              "entity-name": "cnn",
              "parameter-path": "weightsStdDev",
              "val-series": [0.01, "*"]
            },
            {
              "entity-name": "cnn",
              "parameter-path": "layerSparsity",
              "val-series": ["1", "*"]
            },
            {
              "entity-name": "cnn",
              "parameter-path": "layerSparsityLifetime",
              "val-series": ["1", "*"]
            },
            {
              "entity-name": "cnn",
              "parameter-path": "batchSize",
              "val-series": [20, "*"]
            },
            {
              "entity-name": "image-class",
              "parameter-path": "trainingEpochs",
              "val-series": [1]
