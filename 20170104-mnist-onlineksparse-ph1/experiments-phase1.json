{
  "experiments": [
    {
      "import-files": {
        "file-entities": "entities-phase1.json",
        "file-data": [
            "data-phase1.json"
          ]
      },
      "load-local-files": {
        "file-data": []
      },      
      "gen-files": {
        "file-entities": "entities-phase1.json",
        "file-data": [
            "data-phase1.json"
          ]
      },
      "dataset-parameters": [
        {
          "entity-name": "image-class",
          "parameter-path": "sourceFilesPathTraining",
          "value": "training"
        },
        {
          "entity-name": "image-class",
          "parameter-path": "sourceFilesPathTesting",
          "value": "training, testing"
        }
      ],
      "parameter-sweeps": [
        {
          "parameter-set": [
            {
              "entity-name": "autoencoder",
              "parameter-path": "ageMax",
              "val-series": [1000,1000]
            },
            {
              "entity-name": "autoencoder",
              "parameter-path": "sparsityOutput",
              "val-series": [1.5,1.5]
            },
            {
              "entity-name": "autoencoder",
              "parameter-path": "learningRate",
              "val-series": [0.01,0.01]
            },
            {
              "entity-name": "image-class",
              "parameter-path": "trainingEpochs",
              "val-series": [2,4]
            }            
          ]
        }
      ]
    }
  ]
}
